   "source": [
    "with open (\"html.txt\",) as f:\n",
    "    file = f.read()\n",
    "count_tag = 0\n",
    "tags = [\"<\",\">\"]\n",
    "for tag in file:\n",
    "    if tag in tags:\n",
    "        count_tag +=1\n",
    "count = count_tag/2\n",
    "print(count)"
